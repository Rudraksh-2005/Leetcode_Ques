class Solution {
public:
    string frequencySort(string s) {
        unordered_map<char, int> mp; 
        for (char ch : s) {
            mp[ch]++;
        }

        priority_queue<pair<int,char>>pq;

        
        for (auto &it : mp) {
            pq.push({it.second, it.first});
        }
       

        string c=""; 
        while(!pq.empty()){
            c+=string(pq.top().first,pq.top().second);
            pq.pop();
        }

        return c;
    }
};
