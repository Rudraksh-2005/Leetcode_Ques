class Solution {
public:
    bool kLengthApart(vector<int>& nums, int k) {
        int n=nums.size();
        int s=0;
        int e=0;


        for(int i=0;i<n;i++){
            if(nums[i]==1){
                s=i;
                e=s+1;
                break;
            }
        }
        
        while(e<n){
            if(nums[e]==1){
                if(e-(s+1)<k){
                    return false;
                }
                s=e;
            }
            e++;

        }
        return true;
    }
};
