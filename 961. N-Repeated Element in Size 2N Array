class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        unordered_map<int,int>mpp;
        int n=nums.size();
         int ans=0;

        for(int i=0;i<n;i++){
            if(mpp[nums[i]]==1){
                ans=nums[i];
                break;
            }
            else{
                mpp[nums[i]]++;
            }
        }

       

        

        return ans;
    }
};
