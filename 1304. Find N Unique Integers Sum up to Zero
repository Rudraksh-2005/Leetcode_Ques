class Solution {
public:
    vector<int> sumZero(int n) {
        vector<int>ans(n,0);
        int m=n;
        if(n%2==0){
            for(int i=0;i<m/2;i++){
                ans[i]=n/2;
                ans[m-(i+1)]=n/2;
                n=n-2;
            }

            for(int i=0;i<m/2;i++){
                ans[i]=-ans[i];
            }
        }

        else if(n%2){
            
            for(int i=0;i<m/2;i++){
                ans[i]=n/2;
                ans[m-(i+1)]=n/2;
                n=n-2;
            }

            for(int i=0;i<m/2;i++){
                ans[i]=-ans[i];
            }
            
        }

        return ans;
    }
};
