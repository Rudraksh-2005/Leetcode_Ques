class Solution {
public:
    vector<int> replaceNonCoprimes(vector<int>& nums) {
        vector<int>st;
        for(int x:nums){
            int cur=x;
            while(!st.empty()){
                int g=gcd(st.back(),cur);
                if(g==1)break;
                cur=lcm(st.back(),cur);
                st.pop_back();
            }
            st.push_back(cur);
        }

        return st;

    }
};
