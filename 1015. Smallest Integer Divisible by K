class Solution {
public:
    int smallestRepunitDivByK(int k) {
        if(k%2==0 || k%5)return -1;
        long long ans=1;

        while(ans%k!=0){
            ans=ans*10 +1;
        }

        int n=0;
        while(ans!=0){
            ans/=10;
            n++;
        }

        return n;
    }
};
